<div class="funDecs">
<span class="funDec">
    <span class="cm-keyword">function</span>
    <span class="cm-variable"><%= id.name %></span>
    <span class="cm-variable">
    (
        <%= _.map(_.pluck(params, 'name'), function (name) {
            return '<span class="cm-variable">' + name + '</span>';
        }).join(', ') %>
    )
    </span>
</span>
</div>
